{
  "name": "crud-vuex",
  "author": "Julio L. Muller",
  "version": "1.0.1",
  "description": "Simple CRUD application with Vue.js and Vuex.",
  "keywords": [
    "julio muller",
    "crud",
    "vue",
    "vuex"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "0.*",
    "core-js": "2.*",
    "vue": "2.*",
    "vuex": "3.*"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.*",
    "@vue/cli-plugin-eslint": "3.*",
    "@vue/cli-service": "3.*",
    "babel-eslint": "10.*",
    "eslint": "6.*",
    "eslint-plugin-vue": "5.*",
    "vue-template-compiler": "2.*"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
