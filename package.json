{
  "name": "crud-vuex",
  "version": "1.0.10",
  "private": true,
  "license": "MIT",
  "author": "Julio L. Muller",
  "scripts": {
    "start": "vue-cli-service serve --open",
    "build": "vue-cli-service build --dest docs/ --modern",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "~0.21.1",
    "core-js": "^3.8.2",
    "vue": "^3.0.0",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@lacussoft/eslint-config": "^1.0.1",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-vuex": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.5.0"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@lacussoft"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "func-style": [
        "error",
        "declaration",
        {
          "allowArrowFunctions": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
